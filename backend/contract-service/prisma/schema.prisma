generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Contract {
  id        String   @id @default(cuid())
  name      String
  content   String
  status    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
