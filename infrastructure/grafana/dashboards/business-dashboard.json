{
  "id": null,
  "title": "Карта науки - Бизнес метрики",
    "tags": ["business", "science-map", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Пользователи онлайн",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "active_users_total",
            "legendFormat": "Активные пользователи"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Поисковые запросы",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "rate(search_queries_total[1h]) * 3600",
            "legendFormat": "Запросов/час"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Просмотры страниц",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(page_views_total[1h]) * 3600",
            "legendFormat": "Просмотров/час"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5000},
                {"color": "red", "value": 20000}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Время отклика поиска",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(search_duration_seconds_bucket[5m]))",
            "legendFormat": "95-й процентиль (сек)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 2}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Популярные поисковые запросы",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "topk(10, sum by (query) (rate(search_queries_total[24h])))",
            "legendFormat": "{{query}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Запросов/сек"
              }
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "Топ страниц по просмотрам",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "topk(10, sum by (page) (rate(page_views_total[24h])))",
            "legendFormat": "{{page}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Просмотров/сек"
              }
            }
          }
        ]
      },
      {
        "id": 7,
        "title": "Активность пользователей по времени",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(active_users_total[5m])",
            "legendFormat": "Новые пользователи/сек"
          }
        ],
        "yAxes": [
          {
            "label": "Пользователи/сек",
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "Конверсия поиска",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(search_queries_total[5m])",
            "legendFormat": "Поисковые запросы/сек"
          },
          {
            "expr": "rate(search_clicks_total[5m])",
            "legendFormat": "Клики по результатам/сек"
          }
        ],
        "yAxes": [
          {
            "label": "События/сек",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "География пользователей",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum by (country) (rate(active_users_total[1h]))",
            "legendFormat": "{{country}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Пользователей/сек"
              }
            }
          }
        ]
      },
      {
        "id": 10,
        "title": "Ошибки приложения",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(application_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{service}}"
          }
        ],
        "yAxes": [
          {
            "label": "Ошибок/сек",
            "min": 0
          }
        ]
      }
    ]
}
