#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/@changesets+cli@2.29.7_@types+node@20.19.18/node_modules/@changesets/cli/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/@changesets+cli@2.29.7_@types+node@20.19.18/node_modules/@changesets/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/@changesets+cli@2.29.7_@types+node@20.19.18/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/@changesets+cli@2.29.7_@types+node@20.19.18/node_modules/@changesets/cli/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/@changesets+cli@2.29.7_@types+node@20.19.18/node_modules/@changesets/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/@changesets+cli@2.29.7_@types+node@20.19.18/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
