#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/thriftrw@3.11.4/node_modules/thriftrw/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/thriftrw@3.11.4/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/thriftrw@3.11.4/node_modules/thriftrw/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/thriftrw@3.11.4/node_modules:/Users/artemvedernikov/Documents/SNO/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../thriftrw@3.11.4/node_modules/thriftrw/thrift2json.js" "$@"
else
  exec node  "$basedir/../../../../../thriftrw@3.11.4/node_modules/thriftrw/thrift2json.js" "$@"
fi
